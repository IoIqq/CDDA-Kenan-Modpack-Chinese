    "sound": "a faint hissing noise.",
    "volume": 3
  },
  {
    "type": "speech",
    "speaker": "mon_shadow_snake_summoned",
    "sound": "an almost inaudible scraping sound.",
    "volume": 3
  },
  {
    "type": "speech",
    "speaker": "mon_shadow_snake_summoned",
    "sound": "\"Cast aside\u2026\"",
    "volume": 4
  },
  {
    "type": "speech",
    "speaker": "mon_seraphic_shade_fake",
    "sound": "\"Cast aside\u2026\"",
    "volume": 20
  },
  {
    "type": "speech",
    "speaker": "mon_seraphic_shade_fake",
