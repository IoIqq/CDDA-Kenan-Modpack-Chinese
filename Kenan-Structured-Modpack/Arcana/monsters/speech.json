    "volume": 3
  },
  {
    "type": "speech",
    "speaker": "mon_shadow_summoned",
    "sound": "\"Interloper\u2026\"",
    "volume": 4
  },
  {
    "type": "speech",
    "speaker": "mon_shadow_snake_summoned",
